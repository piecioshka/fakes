{
  "id": "videos",
  "type": "array",
  "minItems": 10,
  "mxnItems": 10,
  "uniqueItems": true,
  "items": {
    "type": "object",
    "required": [
      "id",
      "title",
      "description",
      "author",
      "thumbUrl",
      "videoUrl"
    ],
    "properties": {
      "id": {
        "type": "string",
        "chance": {
          "guid": {}
        }
      },
      "title": {
        "type": "string"
      },
      "description": {
        "type": "string",
        "chance": {
          "paragraph": {
            "sentences": 1
          }
        }
      },
      "author": {
        "type": "object",
        "required": ["id", "name", "avatarUrl"],
        "properties": {
          "id": {
            "type": "string",
            "chance": {
              "guid": {}
            }
          },
          "name": {
            "type": "string",
            "faker": {
              "fake": "{{name.firstName}} {{name.lastName}}"
            }
          },
          "avatarUrl": {
            "type": "string",
            "chance": {
              "avatar": {}
            }
          }
        }
      },
      "thumbUrl": {
        "type": "string",
        "faker": {
          "fake": "https://picsum.photos/id/{{random.number(1084)}}/300/150/"
        }
      },
      "videoUrl": {
        "type": "string",
        "faker": {
          "fake": "https://www.youtube.com/embed/{{internet.password(10)}}"
        }
      }
    }
  }
}
