{
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "users",
  "type": "array",
  "minItems": 35,
  "maxItems": 35,
  "items": {
    "type": "object",
    "required": [
      "id",
      "name",
      "avatarUrl",
      "login",
      "password",
      "__DO_NOT_USE__password"
    ],
    "properties": {
      "id": {
        "type": "string",
        "chance": {
          "guid": {}
        }
      },
      "name": {
        "type": "string",
        "faker": {
          "fake": "{{name.firstName}} {{name.lastName}}"
        }
      },
      "avatarUrl": {
        "type": "string",
        "chance": {
          "avatar": {}
        }
      },
      "login": {
        "type": "string",
        "chance": {
          "word": {
            "syllables": 2
          }
        }
      },
      "password": {
        "type": "string",
        "const": "",
        "$comment": "This field will be build with SHA1"
      },
      "__DO_NOT_USE__password": {
        "type": "string",
        "faker": {
          "fake": "{{name.firstName}}{{random.number(100)}}"
        }
      }
    }
  }
}
