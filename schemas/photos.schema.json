{
  "id": "photos",
  "type": "array",
  "minItems": 10,
  "maxItems": 10,
  "items": {
    "type": "object",
    "required": ["id", "createdTime", "author", "description", "imageUrl"],
    "properties": {
      "id": {
        "type": "string",
        "chance": {
          "guid": {}
        }
      },
      "createdTime": {
        "type": "string",
        "faker": {
          "fake": "{{date.past}}"
        }
      },
      "author": {
        "type": "object",
        "required": ["id", "name", "avatarUrl"],
        "properties": {
          "id": {
            "type": "string",
            "chance": {
              "guid": {}
            }
          },
          "name": {
            "type": "string",
            "faker": {
              "fake": "{{name.firstName}} {{name.lastName}}"
            }
          },
          "avatarUrl": {
            "type": "string",
            "chance": {
              "avatar": {}
            }
          }
        }
      },
      "description": {
        "type": "string",
        "chance": {
          "paragraph": {
            "sentences": 1
          }
        }
      },
      "imageUrl": {
        "type": "string",
        "faker": {
          "fake": "https://picsum.photos/id/{{random.number(1084)}}/300/150/"
        }
      }
    }
  }
}
